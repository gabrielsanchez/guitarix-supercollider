
<faust>
	<name>Digital Stereo Delay</name>
	<author></author>
	<copyright></copyright>
	<license></license>
	<version></version>
	<classname>mydsp</classname>
	<inputs>2</inputs>
	<outputs>2</outputs>
	<ui>
		<activewidgets>
			<count>9</count>
			<widget type="hslider" id="1">
				<label>Freeze</label>
				<varname>fHslider0</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>1.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="2">
				<label>bpm</label>
				<varname>fHslider2</varname>
				<init>120.0f</init>
				<min>24.0f</min>
				<max>360.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="vslider" id="3">
				<label>feedback</label>
				<varname>fVslider2</varname>
				<init>50.0f</init>
				<min>1.0f</min>
				<max>100.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="vslider" id="4">
				<label>gain</label>
				<varname>fVslider0</varname>
				<init>100.0f</init>
				<min>0.0f</min>
				<max>120.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="5">
				<label>highpass</label>
				<varname>fHslider5</varname>
				<init>120.0f</init>
				<min>20.0f</min>
				<max>20000.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="6">
				<label>howpass</label>
				<varname>fHslider4</varname>
				<init>12000.0f</init>
				<min>20.0f</min>
				<max>20000.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="vslider" id="7">
				<label>level</label>
				<varname>fVslider1</varname>
				<init>50.0f</init>
				<min>1.0f</min>
				<max>100.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="8">
				<label>mode</label>
				<varname>fHslider3</varname>
				<init>0.0f</init>
				<min>0.0f</min>
				<max>3.0f</max>
				<step>1.0f</step>
			</widget>
			<widget type="hslider" id="9">
				<label>notes</label>
				<varname>fHslider1</varname>
				<init>4.0f</init>
				<min>0.0f</min>
				<max>17.0f</max>
				<step>1.0f</step>
			</widget>
		</activewidgets>
		
		<passivewidgets>
			<count>0</count>
		</passivewidgets>
		
		<layout>
			<group type="vgroup">
				<label>0x00</label>
				<widgetref id="1" />
				<widgetref id="2" />
				<widgetref id="3" />
				<widgetref id="4" />
				<widgetref id="5" />
				<widgetref id="6" />
				<widgetref id="7" />
				<widgetref id="8" />
				<widgetref id="9" />
			</group>
		</layout>
	</ui>
</faust>
